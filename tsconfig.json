{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["element-plus/global"]
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "src/main.js", "electron-main/**/*.ts", "electron-preload/**/*.ts", "electron-main/index.js", "electron-main/index.js", "src/components/util.js", "src/components/util.js", "src/components/bili.js", "src/components/bili.js"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
